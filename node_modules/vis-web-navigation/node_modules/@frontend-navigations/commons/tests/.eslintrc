{
  "env": {
    "jest": true
  },
  "rules": {
    "import/no-extraneous-dependencies": ["off", { "devDependencies": true }],
    "no-unused-expressions": "off",
    "global-require": "off",
    "no-underscore-dangle": "off",
    "security/detect-non-literal-fs-filename": "off",
    "max-len": "off"
  },
  "plugins": [
    "wdio"
  ],
  "extends": "plugin:wdio/recommended"
}